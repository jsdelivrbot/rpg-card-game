{
  "name": "harmony-boilerplate",
  "version": "1.0.0",
  "description": "Harmony Boilerplate is based on Multi Model Layer, MongoDB, Express, React and NodeJS",
  "main": "index.js",
  "repository": "https://github.com/MaxToyberman/awesome-react-boilerplate",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./client/test/test_helper.js --recursive ./client/test/components ",
    "test:watch": "npm run test -- --watch",
    "test:report": "mocha --compilers js:babel-core/register --require ./client/test/test_helper.js --recursive ./client/test/components --reporter mochawesome --reporter-options reportDir=./reports",
    "clear": "rimraf ./client/dist",
    "sass": "gulp sass",
    "sass:watch": "gulp sass:watch",
    "startserver:prod": "cross-env NODE_ENV=production nodemon --watch server/src server/server.js",
    "startserver:dev": "cross-env NODE_ENV=development nodemon --watch server/src server/server.js",
    "apidocs": "cd server && apidoc -i src/api -o docs",
    "build:prod": "cross-env NODE_ENV=production  webpack --watch --config ./webpack.config.prod.js",
    "start:prod": "npm run clear && npm-run-all --parallel sass build:prod startserver:prod test:watch  sass:watch",
    "build:dev": "cross-env NODE_ENV=development  webpack --progress --watch --config ./webpack.config.dev.js",
    "start:dev": "npm run clear && npm-run-all --parallel sass  startserver:dev build:dev  test:watch  sass:watch",
    "start": "npm run start:dev"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "gulp": "^3.9.1",
    "gulp-append-prepend": "^1.0.3",
    "gulp-footer": "^1.0.5",
    "gulp-header": "^1.8.8",
    "gulp-inject": "^4.2.0",
    "gulp-inject-string": "^1.1.0",
    "gulp-remove-files": "0.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-template": "^4.0.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "npm-run-all": "latest",
    "react-addons-test-utils": "^15.2.0",
    "rimraf": "^2.5.2",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-stage-1": "^6.1.18",
    "bcryptjs": "^2.3.0",
    "body-parser": "~1.0.1",
    "browser-sync": "^2.18.8",
    "compression-webpack-plugin": "0.4.0",
    "copy-webpack-plugin": "latest",
    "cross-env": "^4.0.0",
    "crypto-js": "^3.1.8",
    "express": "^4.8.0",
    "guid": "0.0.12",
    "gulp-sass": "^3.1.0",
    "immutable": "^3.8.1",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "mongoose": "~4.6.8",
    "morgan": "~1.0.0",
    "mysql": "^2.13.0",
    "nodemon": "latest",
    "prop-types": "^15.5.8",
    "react": "^15.5.0",
    "react-dom": "^15.5.3",
    "react-intl": "^2.2.3",
    "react-redux": "^4.4.8",
    "react-router": "^3.0.5",
    "react-websocket": "^1.1.7",
    "redux": "^3.6.0",
    "redux-form": "^6.6.2",
    "redux-immutable": "^4.0.0",
    "redux-promise": "^0.5.3",
    "redux-saga": "^0.14.6",
    "rimraf": "^2.6.1",
    "sequelize": "^3.30.4",
    "validator": "^7.0.0",
    "ws": "^2.3.1",
    "ws-reconnect-js": "^1.0.1"
  }
}
